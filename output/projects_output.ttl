reo:72338-fondazione-csp---bando-intelligenza-artificiale-2a-ediz.---ai-leap:-learning-personalization-with-ai-and-of-ai
        rdf:type                       frapo:Project ;
        frapo:hasAcronym               "AI-LEAP" ;
