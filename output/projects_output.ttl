<http://www.semanticweb.org/research-ontology/83563-linking-italian---finanziamento-dell’unione-europea–nextgenerationeu-–-missione-4-componente-2-investimento-1.1---cup-d53d23020720001-->
        rdf:type                       frapo:Project ;
        frapo:hasAcronym               "LiIta" ;
