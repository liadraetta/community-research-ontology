reo:13246-evogamesplus---evolutionary-games-and-population-dynamics:-from-theory-to-applications-call-h2020-msca-itn-h2020
        rdf:type                       frapo:Project ;
        frapo:hasAcronym               "EvoGamesPlus" ;
        frapo:hasEndDate               "28/02/2025" ;
