<http://www.semanticweb.org/research-ontology/16470-system-level-integration-and-functional-characterization-of-circular-rnas-in-precancerous/cancerous-colorectal-lesions>
        rdf:type                       frapo:Project ;
        frapo:hasAcronym               "CRC-CircRNA-MFAG 2021" ;
        frapo:hasEndDate               "30/11/2026" ;
