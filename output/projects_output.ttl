reo:46140-cancelliere-r.--learning-representation-of-spatio-temporal-data-through-deep-neural-networks---bando-grant-for-internationalization---gfi----2022
        rdf:type                       frapo:Project ;
        frapo:hasAcronym               "DeepRep" ;
        frapo:hasEndDate               "22/12/2023" ;
